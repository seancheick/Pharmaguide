# Environment Configuration for PharmaGuide
# Copy this file to .env and fill in your actual values
# Never commit .env to version control

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Service Configuration (Optional - for local development)
EXPO_PUBLIC_HUGGINGFACE_API_KEY=your_huggingface_key_here
EXPO_PUBLIC_GROQ_API_KEY=your_groq_key_here

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# App Configuration
EXPO_PUBLIC_APP_NAME=PharmaGuide
EXPO_PUBLIC_APP_VERSION=1.0.0
